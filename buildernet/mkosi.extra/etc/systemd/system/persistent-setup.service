[Unit]
DefaultDependencies=no
Wants=systemd-udev-settle.service
After=systemd-udev-settle.service tpm2.target
Before=local-fs.target

[Service]
Type=oneshot
RemainAfterExit=yes
Environment=BUILDERNET_TARGET_LUN=__BUILDERNET_TARGET_LUN
ExecStart=/usr/bin/persistent-setup $BUILDERNET_TARGET_LUN
TimeoutStartSec=600

[Install]
WantedBy=local-fs-pre.target
