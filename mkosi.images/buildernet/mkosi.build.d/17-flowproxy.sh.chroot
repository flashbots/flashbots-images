#!/usr/bin/env bash
set -euo pipefail

echo "Installing flowproxy..."
# https://github.com/BuilderNet/FlowProxy/releases
EXPECTED_SHA256=929f9100bcc322fe2de309f6deb2fb87e9b39ebab9280aac8170d09c1799a344
mkdir -p $DESTDIR/usr/bin
curl -fSsL -o $DESTDIR/usr/bin/flowproxy https://github.com/BuilderNet/FlowProxy/releases/download/v1.3.3/flowproxy
echo "${EXPECTED_SHA256}" $DESTDIR/usr/bin/flowproxy | sha256sum --check
chmod +x $DESTDIR/usr/bin/flowproxy
